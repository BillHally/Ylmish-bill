{
  "name": "fable-yjs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "install": "dotnet restore",
    "build:yjs": "dotnet fable src/Fable.Yjs -o build/src/Fable.Yjs",
    "build:yjs-adaptive": "dotnet fable src/Fable.Yjs.Adaptive -o build/src/Fable.Yjs.Adaptive",
    "test+watch": "dotnet fable watch tests/Fable.Yjs.Adaptive.Tests -o build/tests/Fable.Yjs.Adaptive.Tests --run mocha build/tests/Fable.Yjs.Adaptive.Tests/Program.js --watch --parallel"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "mocha": "8.3.2"
  },
  "dependencies": {
    "yjs": "13.5.35"
  }
}
